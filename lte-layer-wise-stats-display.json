  "alertsAndWarnings": {
    "phyAlerts": [
      {
        "condition": "rsrp < -110",
        "severity": "WARNING",
        "message": "Low RSRP - poor signal strength"
      },
      {
        "condition": "sinr < 0",
        "severity": "ERROR", 
        "message": "Negative SINR - severe interference"
      }
    ],
    "macAlerts": [
      {
        "condition": "harq_nack_count > 10",
        "severity": "WARNING",
        "message": "High HARQ NACK rate - poor channel quality"
      }
    ],
    "rlcAlerts": [
      {
        "condition": "retransmissions > 4",
        "severity": "ERROR",
        "message": "Maximum RLC retransmissions exceeded"
      }
    ],
    "rrcAlerts": [
      {
        "condition": "rrc_state == 'IDLE'",
        "severity": "INFO",
        "message": "RRC connection released"
      }
    ],
    "nasAlerts": [
