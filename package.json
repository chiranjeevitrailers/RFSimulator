  "description": "Professional 3GPP Protocol Simulator with 1000+ test cases, real-time analysis, and authentic hardware-like experience for 5G/4G network professionals.",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
