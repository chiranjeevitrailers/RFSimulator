{
  "name": "5glabx-platform",
  "version": "1.0.0",
  "description": "5GLabX - Professional 5G Protocol Simulator & Analysis Platform",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:load": "artillery run load-test.yml",
    "test:e2e": "playwright test",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "audit": "npm audit --audit-level moderate",
    "security": "snyk test",
    "db:generate": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > types/supabase.ts",
    "db:push": "supabase db push",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase migration up",
    "db:seed": "tsx scripts/seed.ts",
    "deploy:staging": "netlify deploy --dir=.next --prod --site=$NETLIFY_STAGING_SITE_ID",
    "deploy:production": "netlify deploy --dir=.next --prod --site=$NETLIFY_PRODUCTION_SITE_ID",
    "postinstall": "prisma generate"
  },
  "dependencies": {
